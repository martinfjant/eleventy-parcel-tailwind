{
  "name": "eleventy-broiler-plate",
  "version": "1.0.0",
  "description": "Eleventy broiler plate with parcel",
  "main": "_site/assets/index.js",
  "author": "Martin Falk Johansson",
  "license": "MIT",
  "private": false,
  "scripts": {
    "prestart": "run-s clean & ELEVENTY_ENV=development eleventy",
    "start": "npm-run-all --parallel dev:*",
    "dev:11ty": "ELEVENTY_ENV=development eleventy --watch",
    "dev:parcel": "parcel serve watch temp/eleventy/index.html --out-dir _site",
    "clean": "rimraf temp"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.8.3",
    "autoprefixer": "^9.6.1",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.3",
    "postcss-modules": "^1.4.1",
    "rimraf": "^3.0.0",
    "tailwind": "^4.0.0"
  }
}
